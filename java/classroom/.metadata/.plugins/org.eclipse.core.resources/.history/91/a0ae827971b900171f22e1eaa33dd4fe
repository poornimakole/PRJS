package com.psl.demo;

public class Calculator {

	public int add(int a,int b) {
		// TODO Auto-generated method stub
        return a+b;
	}
	
	public int divide(int a,int b) {
		// TODO Auto-generated method stub
	int result=0;
    try
    {
    	 result=a/b;
    }
    catch(Exception e)
    {
    	
    }
    	 return result;
	}
}
